{% comment %}

	Carbonclick.com

	version 3.2.0

	Do not modify this file. This file is auto generated and may be overwritten

{% endcomment %}

{% liquid

  assign version = '3.2.0'
  
  assign carbonclick = shop.metafields.carbonclick['settings']
  assign product = all_products['cc-carbon-offset']

  assign variant = product.variants.first


  assign images_path = 'https://shopify-web.carbon.click/images/'
  assign logo_image_path = images_path | append: 'carbonclick-logo-' | append: carbonclick.carbonclick_logo | append: '.svg'


  
  if type == 'compressed' or type == 'mini'
    assign is_full_widget = false
  else
    assign is_full_widget = true
  endif
  
  
  assign cart_item = cart.items | where: 'product_id', product.id | first

  if cart_item != blank
    assign offset_added = true
  else
    assign offset_added = false
  endif
    
  assign widget_enabled = carbonclick.enabled

  assign offset_index = carbonclick.offset_amount | minus: 1


  if money_mode == 'decimal'
    assign offset_price = variant.price | money
  else
    assign offset_price = variant.price | money_without_trailing_zeros
  endif

  if offset_added
    unless type == 'mini'
      assign button_text = carbonclick.thank_you_text | default: 'Thank you!'
    endunless
  else
	  assign button_text = offset_price | prepend: '<span class="money">' | append: '</span>'
  endif


  if product.metafields.carbonclick.impact

    if carbonclick.show_merchant_impact == true
  	  assign impact = product.metafields.carbonclick.merchant_impact
    else
	    assign impact = product.metafields.carbonclick.impact
    endif
    
    assign impact_unit = impact.carbonOffsetImpact['unit']

    assign impact_value_kg = impact.carbonOffsetImpact['value']


    case variant.weight_unit
      when 'lb' or 'oz'
        assign impact_value_local = impact_value_kg | times: 2.204620 | round
        assign impact_unit = 'lb'
      when 'kg' or 'g'
        assign impact_value_local = impact_value_kg | round
        assign impact_unit = 'kg'
      else
        assign impact_value_local = impact_value_kg | round
        assign impact_unit = 'kg'
    endcase

  endif


  

  
%}




