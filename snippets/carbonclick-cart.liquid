<style>
  
  
  #cc-offset-widget {
    display: none;
    visibility: hidden;
    opacity: 0;
    position: relative;
    width: 100%;
    max-width: 100%;
    margin: 20px 0;
    padding: 0;
    background: {{ carbonclick.plugin_background_colour }};
    border: 1px solid {{ carbonclick.plugin_border_colour }};
    border-radius: 4px;
  }
    
   #cc-offset-widget.is-enabled {
      display: block;
      visibility: visible;
      opacity: 1;
   }
    
    #cc-offset-widget .svg-icon {
      display: inline-block;
    }
    
    .cc-tagline .svg-icon {
      width: 35px;
      margin-right: 1px;
      vertical-align: middle;
    }
    
    
  
  #cc-add-button {
    width: 100%;
    height: 45px;
    line-height: 1;
    font-weight: bold;
    border: 2px solid {{ carbonclick.button_border_colour }} !important;
    background: {{ carbonclick.button_background_colour }};
    color: {{ carbonclick.button_text_colour }};
    padding: 8px 24px;
    border-radius: 3px;
  	transition: all .5s ease-out;
    position: relative;
    z-index: 1;
    box-sizing: border-box;
    margin-bottom: 0;    
  }
  
  #cc-add-button:after {
    content: '';
    position: absolute;
    z-index: -1;
    width: 0%;
    height: 100%;
    top: 0;
    left: 0;
    background: #fff;
    transition: all 0.6s;
  }
  
  #cc-add-button:hover:after {
    width: 100%;
  }
  
  #cc-add-button:hover {
    color: {{ carbonclick.button_background_colour }}!important;
  }
  
  #cc-add-button.cc-add-button--active {
    background: {{ carbonclick.button_background_colour_selected }};
    border: 1px solid {{ carbonclick.button_background_colour }} !important;
    color: {{ carbonclick.button_text_colour_selected }};
    margin-bottom: 0;
  }


  .cc-add-button-tick {
    display: none;
    color: {{ carbonclick.button_checkmark_icon_selected }};
  }

  .cc-add-button-plus {
    display: inline-block;
    color: {{ carbonclick.button_text_colour }};
  }

  .cc-add-button--active .cc-add-button-plus {
    display: none;
  }

  .cc-add-button--active .cc-add-button-tick {
    display: inline-block;
  }
  
  
  .cc-content {
    display: grid;
    grid-template-columns: auto 1fr auto 200px;
    grid-gap: 5px 30px;
    align-items: center;
    padding: 10px 20px;
    max-width: 100%;
  }

  
    
  .cc-content .svg-icon {
    width: 55px;
    height: 50px;
    margin: 0 auto;
  }
  
  .cc-content .svg-icon svg{
    width: auto;
    height: auto;
  }
  
  .cc-content-title {
    font-weight: 500;
    font-size: 15px;
    color: {{ carbonclick.plugin_text_colour_top_section }};
    max-width: 300px;
  }
  
  
  
  #cc-learn-more {
    color: {{ carbonclick.plugin_text_colour_top_section }};
    font-weight: 500;
    text-decoration: none;
    font-size: initial;
  }
  
  .cc-link, .cc-link:active, .cc-link:hover, .cc-link:visited {
    color: {{ carbonclick.plugin_text_colour_top_section }} !important;
  }
  

  .cc-logo {
    display: inline-block;
    height: 20px;
    width: 120px;
  }

  
  
  #cc-dropdown {
    display: none;
    min-height: 200px;
    transition: all 0.6s ease-in-out;
    
    margin: 0 20px 0 20px;
    
  }
  
    
  .cc-dropdown-body {
    display: grid;
    grid-template-columns: 1fr 1fr;
    grid-gap: 30px 80px;
    border-radius: 5px;
    background-color: {{ carbonclick.plugin_background_colour_expanded }};
    padding: 28px;
    color: {{ carbonclick.plugin_small_text_colour_expanded }};

  }

  .cc-dropdown-body p {
    color: {{ carbonclick.plugin_small_text_colour_expanded }};
  }

  .cc-dropdown-bottom {
    padding: 15px;
    text-align: right;
  }
      
  
  
  #cc-dropdown h5 {
    margin-bottom: 40px;
    font-size: 18px !important;
    color: {{ carbonclick.plugin_large_text_colour_expanded }};
  }
  
  #cc-dropdown.is--active {
    display: block;
  }
  
  
  .cc-help-fund {
    display: flex;
    background-color: transparent;
  }
  
  .cc-help-fund__item {
    display: block;
    text-align: center;
    font-size: 14px;
    color: {{ carbonclick.plugin_text_colour_expanded_section }};
    margin-bottom: 20px;
  }
    
    
  .cc-help-fund__item svg {
  	height: 65px;
    margin-bottom: 10px;
    width: 100%;
  }
  .cc-help-fund__item svg, .cc-stats svg {
    fill: none;
  }

  .cc-stats {
    list-style: outside none;
    padding-left: 0;
  }
  
  
  ul.cc-stats .cc-stat {
    display: grid !important;
    grid-template-columns: 1fr auto;
    grid-gap: 5px 20px;
    margin-bottom: 15px !important;
    width: 100% !important;
  }
  
  
  .cc-stat-name {
      font-size: 15px;
      color: {{ carbonclick.plugin_text_colour_expanded_section }};
  }
    
    .cc-stat-name .svg-icon {
      width: 35px;
      margin-right: 15px;
      vertical-align: middle;
    }
  
  
  .cc-stat-number {
    color: {{ carbonclick.plugin_large_text_colour_expanded }};
    font-size: 20px;
    line-height: 37px;
    font-weight: 600;
  }
 
    
    
  
    
    
    .cc-tagline {
      margin-bottom: 30px;
      font-size: 15px;
      color: {{ carbonclick.plugin_small_text_colour_expanded }};
    }
   
  	.cc-tagline img {
      width: 25px;
      vertical-align: middle;
      margin-right: 10px;
    }
    
    
    .cc-logo-inline {
      display: inline-block;
      height: 20px !important;
      margin-left: 5px;
      vertical-align: middle;
    }
    
    .cc-button-add {
      position: absolute;
      top: 14px;
      right: 10px;
      margin-top: 0;
      width: auto;
      height: auto;
    }
    
    .cc-angle-down {
     
      border: solid {{ carbonclick.plugin_icons_colour }};
      border-width: 0 3px 3px 0;
      display: inline-block;
      padding: 4px;
      margin-left: 5px;
      margin-bottom: 8px;
      vertical-align: middle;
      transform: rotate(45deg);
      -webkit-transform: rotate(45deg);
    }


    
    @media only screen and (max-width: 48em) {

      .cc-content {
        grid-template-columns: 1fr;
      }

      .cc-dropdown-body {
        grid-template-columns: 1fr;
      }

      .cc-help-fund {
        flex-wrap: wrap;
		    justify-content: center;
      }

      .cc-stats .cc-stat {
        grid-template-columns: 1fr !important;
        justify-content: center;
        margin-bottom: 30px !important;
      }


      .cc-dropdown-bottom {
        text-align: center;
      }
      
      .cc-content-title {
        margin: 0 0 10px 0;
        text-align: center;
        max-width: 100%;
      }
      
      
      .cc-help-fund__item {
        margin-bottom: 30px;
        text-align: center;
      }
      
      
      .cc-offset-bottom, .cc-tagline {
        text-align: center;
      }
      
      .cc-tagline {
        margin-top: 20px;
      }
      
      #cc-dropdown h5 {
        text-align: center;
      }
      
      .cc-stat-number, .cc-stat-name {
        display: block;
        text-align: center;
        float: none;
      }
      
      .cc-hidden-mobile {
        display: none;
      }
      
      #cc-learn-more {
        display: block;
        margin-bottom: 20px;
        text-align: center;
      }
      
      
      .cc-stat-number {
        line-height: 1em;
      }
    }


    [data-climate-friendly-type="mini"]#cc-offset-widget, [data-climate-friendly-type="compressed"]#cc-offset-widget {
      display: block;
    }


    [data-climate-friendly-type="mini"]#cc-offset-widget {
      margin: 5px 0;
    }


    
    
    [data-climate-friendly-type="mini"] .cc-content {
      grid-template-columns: 1fr auto;
      grid-gap: 5px 12px;
      text-align: left;
      padding: 6px 12px;
    }

    [data-climate-friendly-type="compressed"] .cc-content {
      grid-template-columns: 1fr;
      text-align: center;
    }
    
    [data-climate-friendly-type="mini"] .cc-content-title {
      max-width: 100%;
      text-align: left !important;
      font-size: 13px;
    }

    [data-climate-friendly-type="compressed"] .cc-content-title {
      max-width: 100%;
      text-align: center !important;
      
    }


    [data-climate-friendly-type="mini"] #cc-add-button, [data-climate-friendly-type="compressed"] #cc-add-button {
      line-height: 20px;
      height: auto;
      padding: 4px 14px;
    }

    [data-climate-friendly-type="mini"] .cc-logo-inline {
      height: 16px;
    }
  
</style>




<div 
  id="cc-offset-widget"
  class="{{ type }}"
  data-controller="climate-friendly"
  data-climate-friendly-placement="{{ placement }}"
  data-climate-friendly-meta="{{ product.metafields.carbonclick['meta'] | json | escape }}"
  data-climate-friendly-placement-position="{{ position | default: 'beforebegin' }}"
  data-climate-friendly-shop="{{ shop.permanent_domain }}"
  data-climate-friendly-enabled="{{ carbonclick.enabled }}"
  data-climate-friendly-type="{{ type }}"
  data-climate-friendly-cart-count="{{ cart.item_count }}"
  data-climate-friendly-variant-id="{{ variant.id }}"
  data-climate-friendly-delay="{{ delay | default: 0 }}"
  data-climate-friendly-open-on-reload="{{ open_on_reload }}"
  data-climate-friendly-is-offset-added="{% if offset_added %}true{% else %}false{% endif %}"
>
  
  <div class="cc-content">

    {% if is_full_widget %}
      {{ cloud_svg }}
    {% endif %}
    
    
    <div class="cc-content-title">
      {{ carbonclick.headline_text | default: 'Reduce the carbon footprint of your purchase with' }} <a href="https://carbonclick.com" target="_blank">{{ logo_image_path | img_tag: 'carbonclick', 'cc-logo-inline' }}</a>
    </div>
    
    
    {% if is_full_widget %}
      <a id="cc-learn-more" href="#" class="cc-link" data-action="click->climate-friendly#more">{{ carbonclick.learn_more_text | default: 'Learn More' }} <i class="cc-angle-down"></i></a>
    {% endif %}


    <button id="cc-add-button"
            type="button"
            data-action="click->climate-friendly#toggleCart"
            class="{% if offset_added %}cc-add-button--active{% endif %}"
            >
              
              <span class="cc-add-button-tick">{{ green_tick_thanks_svg }}</span>
              <span class="cc-add-button-plus">{{ plus_svg }}</span>
              
              
                {{ button_text }}
              

    </button>
  </div>
  
  {% unless type == 'compressed' or type == 'mini' %}
  <div id="cc-dropdown" data-target="climate-friendly.dropdown">
    <div class="cc-dropdown-body">
      <div class="cc-projects-container">

        <div class="cc-tagline">
          {{ green_tick_svg }}
          {{ carbonclick.projects_text | default: 'Support projects that fight climate change.' }}
        </div>

        <h5>{{ carbonclick.fund_text | default: 'Help fund:' }}</h5>


        <div class="row cc-help-fund">
          <div class="col-xs-12 col-sm-5 cc-help-fund__item">
            {{ planter_svg }}
            <div>{{ carbonclick.project_1_text | default: 'Native Forest Regeneration' }}</div>
          </div>

          <div class="col-xs-12 col-sm-5 cc-help-fund__item">
            {{ windmill_svg }}
            <div>{{ carbonclick.project_2_text | default: 'International Renewable Energy Projects' }} </div>
          </div>
        </div>

      </div>
      <div class="cc-stats-container">
        <div class="cc-tagline">
          {{ green_tick_svg }}
          {{ carbonclick.protecting_text | default: 'Play a part in protecting our planet.' }}
        </div>
        {% if product.metafields.carbonclick.impact %}
        <h5>{{ carbonclick.movement_text | default: 'Add to our movement:' }}</h5>
        <ul class="cc-stats">
          
          
          <li class="cc-stat">
            <span class="cc-stat-name">{{ contributors_svg }} {{ carbonclick.contributors_text | default: 'Contributors' }}</span>
            <span class="cc-stat-number">{{ impact.contributorImpact['value'] | times: 100 | money_without_currency | remove: '.00' }}</span>
          </li>

          <li class="cc-stat">
            <span class="cc-stat-name">{{ tree_svg }} {{ carbonclick.trees_planted_text | default: 'Trees Planted' }}</span>
            <span class="cc-stat-number">{{ impact.treeImpact['value'] | times: 100 | money_without_currency | remove: '.00' }}</span>
          </li>

          <li class="cc-stat">
            <span class="cc-stat-name">{{ cloud_svg_2 }} {{ carbonclick.co2_saved_text | default: 'CO2 Saved' }} ({{ impact_unit }})</span>
            <span class="cc-stat-number">{{ impact_value_local | times: 100 | money_without_currency | remove: '.00' }}</span>
          </li>
        </ul>
        {% endif %}

      </div>
    </div>
    <div class="cc-dropdown-bottom">
      {{ carbonclick.powered_by_text | default: 'Powered by' }} {{ logo_image_path | img_tag: 'carbonclick', 'cc-logo-inline' }}
    </div>
  </div>    
  {% endunless %} 
</div>
{% endunless %}



{% endif %}

